{
  "name": "audit-logging",
  "description": "Listens and responds to audit events",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "@aws-sdk/client-lambda": "^3.250.0",
    "@babel/core": "^7.20.12",
    "@types/aws-lambda": "^8.10.109",
    "@types/jest": "^29.2.5",
    "@types/lodash.groupby": "^4.6.7",
    "@types/lodash.maxby": "^4.6.7",
    "@types/lodash.minby": "^4.6.7",
    "@types/lodash.partition": "^4.6.7",
    "@types/node": "^18.11.18",
    "@types/pg": "^8.6.6",
    "@types/stompit": "^0.26.3",
    "@types/uuid": "^9.0.0",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "esbuild": "^0.16.16",
    "eslint": "^8.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lambda-local": "^2.0.3",
    "lint-staged": "^13.1.0",
    "lodash.partition": "^4.6.0",
    "mockdate": "^3.0.5",
    "npm-check-updates": "^16.6.2",
    "prettier": "^2.8.3",
    "serverless": "^3.26.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-esbuild": "^1.37.0",
    "serverless-hooks": "^1.0.0",
    "serverless-offline": "^12.0.4",
    "serverless-s3-local": "^0.7.1",
    "shelljs": "^0.8.5",
    "ts-jest": "^29.0.5",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4",
    "wait-on": "^7.0.1"
  },
  "scripts": {
    "build": "node build.js",
    "check": "tsc --noEmit",
    "hook:before:offline:start:init": "make run-mq run-pg",
    "install-dynamo": "if [ ! -d ./.dynamodb ]; then sls dynamodb install; fi",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "start-api": "npm run install-dynamo && sls offline start --httpPort 3010",
    "test:e2e": "jest --runInBand --testPathPattern=e2e",
    "test:integration": "jest --runInBand --testPathPattern=integration --testPathIgnorePatterns='e2e'",
    "test:unit:watch": "jest --watchAll --testPathIgnorePatterns='integration' 'e2e'",
    "test:unit": "jest --testPathIgnorePatterns='integration' 'e2e'",
    "test": "jest --runInBand  --namePattern=",
    "update-deps": "ncu -x esbuild -u && npm install",
    "wait-for-api": "wait-on tcp:3010"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1295.0",
    "axios": "^1.2.3",
    "date-fns": "^2.29.3",
    "lodash.groupby": "^4.6.0",
    "lodash.maxby": "^4.6.0",
    "lodash.minby": "^4.6.0",
    "pg": "^8.8.0",
    "pino": "^8.8.0",
    "stompit": "^1.0.0",
    "uuid": "^9.0.0",
    "xml-formatter": "^3.2.0",
    "xml2js": "^0.4.23",
    "zod": "^3.20.2"
  }
}
