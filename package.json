    "serverless-offline": "^13.9.0",
    "serverless-s3-local": "^0.8.5",
    "shelljs": "^0.10.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "wait-on": "^9.0.3",
    "ws": "^8.19.0"
  },
  "scripts": {
    "build": "node build.js",
    "check": "tsc --noEmit",
    "hook:before:offline:start:init": "make run-mq run-pg",
    "destroy": "docker-compose -f ./environment/docker-compose.yml down",
    "install-dynamo": "./scripts/install-dynamodb-local.sh && make fix-serverless-dynamodb-local",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "start-api": "npm run install-dynamo && sls offline start --httpPort 3010",
    "test:e2e": "jest --runInBand --testPathPattern=e2e",
    "test:integration": "jest --runInBand --testPathPatterns=integration --testPathIgnorePatterns='e2e'",
    "test:unit:watch": "jest --watchAll --testPathIgnorePatterns='integration' 'e2e'",
    "test:unit": "jest --update-snapshot --testPathIgnorePatterns='integration' 'e2e'",
    "test": "jest --runInBand  --namePattern=",
    "update-deps": "ncu -u && npm install",
    "prepare": "husky",
    "wait-for-api": "wait-on tcp:3010"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": "eslint --cache --fix --max-warnings 0 --ignore-pattern !.ncurc.js"
  },
  "dependencies": {
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1693.0",
    "axios": "^1.13.2",
    "date-fns": "^4.1.0",
    "lodash.groupby": "^4.6.0",
    "lodash.maxby": "^4.6.0",
