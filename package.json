{
  "name": "audit-logging",
  "description": "Listens and responds to audit events",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "@aws-sdk/client-lambda": "^3.370.0",
    "@babel/core": "^7.22.9",
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.5.3",
    "@types/express": "^4.17.17",
    "@types/lodash.groupby": "^4.6.7",
