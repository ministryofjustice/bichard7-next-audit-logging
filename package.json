    "@types/lodash.minby": "^4.6.7",
    "@types/lodash.partition": "^4.6.7",
    "@types/node": "^20.4.4",
    "@types/pg": "^8.10.2",
    "@types/stompit": "^0.26.3",
    "@types/uuid": "^9.0.2",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "body-parser": "^1.20.2",
    "convert-lambda-to-express": "^1.3.1",
    "esbuild": "^0.16.16",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "lambda-local": "^2.1.1",
    "lint-staged": "^13.2.3",
    "lodash.partition": "^4.6.0",
    "mockdate": "^3.0.5",
    "npm-check-updates": "^16.10.16",
    "prettier": "^3.0.0",
    "serverless": "^3.33.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-esbuild": "^1.46.0",
    "serverless-hooks": "^1.0.0",
    "serverless-offline": "^12.0.4",
    "serverless-s3-local": "^0.7.2",
    "shelljs": "^0.8.5",
    "ts-jest": "^29.1.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "ts-node": "^10.9.1",
    "wait-on": "^7.0.1"
  },
  "scripts": {
    "build": "node build.js",
    "check": "tsc --noEmit",
    "hook:before:offline:start:init": "make run-mq run-pg",
    "install-dynamo": "./scripts/install-dynamodb-local.sh && make fix-serverless-dynamodb-local",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "start-api": "npm run install-dynamo && sls offline start --httpPort 3010",
    "test:e2e": "jest --runInBand --testPathPattern=e2e",
    "test:integration": "jest --runInBand --testPathPattern=integration --testPathIgnorePatterns='e2e'",
    "test:unit:watch": "jest --watchAll --testPathIgnorePatterns='integration' 'e2e'",
    "test:unit": "jest --testPathIgnorePatterns='integration' 'e2e'",
    "test": "jest --runInBand  --namePattern=",
    "update-deps": "ncu -x esbuild -u && npm install",
    "wait-for-api": "wait-on tcp:3010"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1420.0",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "lodash.groupby": "^4.6.0",
    "lodash.maxby": "^4.6.0",
    "lodash.minby": "^4.6.0",
    "pg": "^8.11.1",
    "pino": "^8.14.1",
    "stompit": "^1.0.0",
    "uuid": "^9.0.0",
    "xml-formatter": "^3.4.1",
    "xml2js": "^0.6.0",
    "zod": "^3.21.4"
  }
