{
  "name": "audit-log-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup:env": "cd ../audit-log-api && npm run start && cd - && ./scripts/setup-environment-variables.sh",
    "dev": "npm run setup:env && next dev",
    "dev:mocked": "./scripts/setup-environment-variables.sh mocked && NODE_OPTIONS='--require \"./mocks/index\"' next dev",
    "dev:skip-env": "./scripts/setup-environment-variables.sh && next dev",
    "build": "next build",
    "start": "next start",
    "start:mocked": "./scripts/setup-environment-variables.sh mocked && NODE_OPTIONS='--require \"./mocks/index\"' next start",
    "test": "../scripts/test.sh",
    "test:ci": "../scripts/test.sh ci",
    "test:unit": "../scripts/test.sh unit",
    "host": "npm run build && nodemon scripts/host/start.mjs",
    "cypress:open": "cypress open",
    "test:ui": "concurrently --kill-others --success first \"npm run start:mocked\" \"./scripts/run-ui-tests.sh\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@material-ui/core": "^4.11.4",
    "@material-ui/lab": "*",
    "classnames": "^2.2.6",
    "date-fns": "^2.20.2",
    "next": "^11.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-infinite-scroll-component": "^6.1.0",
    "restana": "^4.8.1",
    "serve-static": "^1.14.1",
    "serverless-http": "^2.7.0",
    "shared": "file:../shared",
    "styled-components": "^5.3.0",
    "swr": "^0.5.6",
    "use-http": "^1.0.22"
  },
  "devDependencies": {
    "@aws-sdk/client-api-gateway": "^3.13.0",
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "concurrently": "^6.0.2",
    "cypress": "^6.8.0",
    "http-lambda-proxy": "^1.1.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "jest-styled-components": "^7.0.4",
    "mockdate": "^3.0.5",
    "msw": "^0.28.1",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3",
    "typescript-plugin-styled-components": "^1.5.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
