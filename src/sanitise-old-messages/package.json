{
  "name": "sanitise-old-messages",
  "version": "1.0.0",
  "description": "A lamdba that sanitises (removes PII from) audit log messages older than a threshold",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && webpack --config ./.webpack/webpack.prod.js",
    "build:dev": "npm run clean && webpack --config ./.webpack/webpack.dev.js",
    "test": "jest --runInBand",
    "test:unit": "jest --testPathIgnorePatterns='integration' 'e2e'",
    "test:unit:watch": "jest --watchAll --testPathIgnorePatterns='integration' 'e2e'",
    "test:e2e": "jest --testPathPattern=e2e --runInBand"
  },
  "dependencies": {
    "shared": "file:../shared",
    "shared-types": "file:../shared-types"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.0",
    "@types/pg": "^8.6.5",
    "babel-loader": "^8.2.5",
    "date-fns": "^2.29.3",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "jest": "^29.2.1",
    "lambda-local": "^2.0.3",
    "pg": "^8.8.0",
    "rimraf": "^3.0.2",
    "shared-testing": "file:../shared-testing",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ministryofjustice/bichard7-next-audit-logging.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ministryofjustice/bichard7-next-audit-logging/issues"
  },
  "homepage": "https://github.com/ministryofjustice/bichard7-next-audit-logging#readme"
}
