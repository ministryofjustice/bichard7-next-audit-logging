{
  "name": "audit-log-api",
  "version": "1.0.0",
  "description": "The API for interacting with Audit Log entries",
  "main": "index.js",
  "scripts": {
    "build": "rimraf build && webpack --config ./.webpack/webpack.prod.js",
    "build:dev": "rimraf build && webpack --config ./.webpack/webpack.dev.js",
    "test": "jest --runInBand",
    "test:unit": "jest --testPathIgnorePatterns='integration' 'e2e'",
    "test:unit:watch": "jest --watchAll --testPathIgnorePatterns='integration' 'e2e'",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "start": "sls offline start --httpPort 3010",
    "wait-for-api": "wait-on tcp:3010",
    "hook:before:offline:start:init": "make -C ../.. run-mq",
    "install-dynamo": "sls dynamodb install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ministryofjustice/bichard7-next-services.git"
  },
  "author": "Made Tech Ltd",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ministryofjustice/bichard7-next-services/issues"
  },
  "homepage": "https://github.com/ministryofjustice/bichard7-next-services#readme",
  "dependencies": {
    "aws-lambda": "^1.0.7",
    "axios": "^0.26.1",
    "shared": "file:../shared",
    "shared-types": "file:../shared-types",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/aws-lambda": "^8.10.93",
    "@types/jest": "^27.4.1",
    "@types/uuid": "^8.3.4",
    "babel-loader": "^8.2.4",
    "fork-ts-checker-webpack-plugin": "^7.2.1",
    "jest": "^27.5.1",
    "jest-junit": "^13.0.0",
    "rimraf": "^3.0.2",
    "serverless": "^3.10.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-hooks": "^1.0.0",
    "serverless-offline": "^8.5.0",
    "serverless-plugin-typescript": "^2.1.1",
    "serverless-s3-local": "^0.6.22",
    "shared-testing": "file:../shared-testing",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3",
    "wait-on": "^6.0.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  }
}
