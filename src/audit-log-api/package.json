{
  "name": "audit-log-api",
  "version": "1.0.0",
  "description": "The API for interacting with Audit Log entries",
  "main": "index.js",
  "scripts": {
    "build": "rimraf build && webpack --config ./.webpack/webpack.prod.js",
    "build:dev": "rimraf build && webpack --config ./.webpack/webpack.dev.js",
    "test": "jest --runInBand --namePattern=",
    "test:unit": "jest --testPathIgnorePatterns='integration' 'e2e'",
    "test:unit:watch": "jest --watchAll --testPathIgnorePatterns='integration' 'e2e'",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --runInBand --testPathPattern=e2e",
    "start": "sls offline start --httpPort 3010",
    "wait-for-api": "wait-on tcp:3010",
    "hook:before:offline:start:init": "make -C ../.. run-mq && make -C ../.. run-pg",
    "install-dynamo": "sls dynamodb install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ministryofjustice/bichard7-next-services.git"
  },
  "author": "Made Tech Ltd",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ministryofjustice/bichard7-next-services/issues"
  },
  "homepage": "https://github.com/ministryofjustice/bichard7-next-services#readme",
  "dependencies": {
    "@types/lodash.maxby": "^4.6.7",
    "@types/lodash.minby": "^4.6.7",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1232.0",
    "axios": "^1.1.2",
    "date-fns": "^2.29.3",
    "lodash.maxby": "^4.6.0",
    "lodash.minby": "^4.6.0",
    "shared": "file:../shared",
    "shared-types": "file:../shared-types",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@types/aws-lambda": "^8.10.106",
    "@types/jest": "^29.1.2",
    "@types/uuid": "^8.3.4",
    "babel-loader": "^8.2.5",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "jest": "^29.1.2",
    "jest-junit": "^14.0.1",
    "mockdate": "^3.0.5",
    "rimraf": "^3.0.2",
    "serverless": "^3.22.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-hooks": "^1.0.0",
    "serverless-offline": "^11.1.1",
    "serverless-plugin-typescript": "^2.1.4",
    "serverless-s3-local": "^0.6.22",
    "shared-testing": "file:../shared-testing",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4",
    "wait-on": "^6.0.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
