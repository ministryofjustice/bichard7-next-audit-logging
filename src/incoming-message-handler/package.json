  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/plugin-bugfix-v8-spread-parameters-in-optional-chaining": "^7.18.9",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
