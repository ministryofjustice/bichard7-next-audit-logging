{
  "name": "audit-log-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "init:audit-log-api": "cd ../audit-log-api && npm run install-dynamo && npm run start",
    "setup:env": "./scripts/setup-environment-variables.sh",
    "dev": "npm run setup:env && next dev -p 3020",
    "dev:mocked": "./scripts/setup-environment-variables.sh mocked && NODE_OPTIONS='--require \"./mocks/index\"' next dev -p 3020",
    "dev:skip-env": "./scripts/setup-environment-variables.sh && next dev -p 3020",
    "build": "next build",
    "start": "next start",
    "start:mocked": "./scripts/setup-environment-variables.sh mocked && NODE_OPTIONS='--require \"./mocks/index\"' next start -p 3020",
    "test": "jest --testPathIgnorePatterns=cypress",
    "test:unit": "jest --testPathIgnorePatterns=cypress",
    "host": "npm run build && nodemon scripts/host/start.mjs",
    "cypress:open": "cypress open",
    "test:integration": "concurrently --kill-others --success first \"npm run start:mocked\" \"./scripts/run-integration-tests.sh\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@material-ui/core": "^4.11.4",
    "@material-ui/lab": "*",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "next": "^12.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-infinite-scroll-component": "^6.1.0",
    "restana": "^4.9.3",
    "serve-static": "^1.14.2",
    "serverless-http": "^2.7.0",
    "shared": "file:../shared",
    "shared-types": "file:../shared-types",
    "styled-components": "^5.3.3",
    "swr": "^1.2.1",
    "use-http": "^1.0.26"
  },
  "devDependencies": {
    "@aws-sdk/client-api-gateway": "^3.49.0",
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.0",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/react": "^6.4.18",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.15",
    "@types/react": "^17.0.39",
    "babel-jest": "^27.5.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-styled-components": "^2.0.2",
    "concurrently": "^7.0.0",
    "cypress": "^9.4.1",
    "eslint-config-airbnb": "^19.0.4",
    "http-lambda-proxy": "^1.1.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.0",
    "jest-junit": "^13.0.0",
    "jest-styled-components": "^7.0.8",
    "mockdate": "^3.0.5",
    "msw": "^0.36.8",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5",
    "typescript-plugin-styled-components": "^2.0.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
