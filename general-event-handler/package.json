{
  "name": "general-event-handler",
  "version": "1.0.0",
  "description": "Receives events from the Bichard system and attaches to Audit Log records",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && webpack --config ./.webpack/webpack.prod.js",
    "build:dev": "npm run clean && webpack --config ./.webpack.dev.js",
    "setup:env": "cd ../environment && ./setup.sh && cd -",
    "test": "npm run setup:env && ../scripts/test.sh",
    "test:unit": "../scripts/test.sh unit",
    "test:integration": "npm run setup:env && ../scripts/test.sh integration",
    "test:e2e": "npm run setup:env && ../scripts/test.sh e2e",
    "test:ci": "../scripts/test.sh ci",
    "start": "npm run build:dev && npm run setup:env && ./scripts/deploy-infrastructure.sh"
  },
  "dependencies": {
    "aws-lambda": "^1.0.6",
    "shared": "file:../shared"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@types/aws-lambda": "^8.10.73",
    "@types/jest": "^26.0.21",
    "babel-loader": "^8.2.2",
    "fork-ts-checker-webpack-plugin": "^6.2.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3",
    "webpack": "^5.27.2",
    "webpack-cli": "^4.5.0"
  }
}
